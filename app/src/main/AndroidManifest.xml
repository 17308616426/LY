        <activity android:name=".Register_Login.Bind_Phone" />
        <activity android:name=".Register_Login.Register_Phone" />
        <activity android:name=".Register_Login.Register_Account" />
        <activity android:name=".Register_Login.Register_Way" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>